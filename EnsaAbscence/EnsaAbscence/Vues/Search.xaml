             x:Class="EnsaAbscence.Vues.Search"
             xmlns:local="clr-namespace:EnsaAbscence"
>
    <ContentPage.Content>
        <StackLayout >
            <Picker BackgroundColor="Transparent" Margin="0,5,0,5"  x:Name="filiereAnne" Title="Filière">
            </Picker>
            <Picker BackgroundColor="Transparent" Margin="0,5,0,5" x:Name="AnnePicker" Title="Annee" IsEnabled="False" >

            </Picker>
            <local:myEntry x:Name="nomEtudiant" Placeholder="Nom Etudiant..."></local:myEntry>
            <ListView >
                <ListView.ItemTemplate>
                    <DataTemplate>
                        <ViewCell>
                            
                        </ViewCell>
                    </DataTemplate>
                </ListView.ItemTemplate>
            </ListView>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>